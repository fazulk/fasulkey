{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Fasulkey Server: 9889",
            "address": "0.0.0.0",
            "port": 9889,
            "internalConsoleOptions": "neverOpen",
            "restart": true,
            "localRoot": "${cwd}",
            "remoteRoot": "${cwd}",
            "skipFiles": [
                "${cwd}/node_modules/**/*.js",
                "${cwd}/lib/**/*.js",
                "<node_internals>/**/*.js"
            ]
        }
    ]
}
